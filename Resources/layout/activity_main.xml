<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView1"
        android:layout_centerInParent="false"
        android:layout_centerVertical="false"
        android:scrollIndicators="none"
        android:scrollbars="none">


        <RelativeLayout
            android:id="@+id/containerscroll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/abc_action_bar_default_height_material">

            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/toplayout"
                android:fadingEdge="none"
                android:textDirection="ltr"
                android:layoutDirection="ltr"
                android:padding="15dp"
                android:layout_marginBottom="0.0dp"
                android:background="@mipmap/homebg2"
                android:layout_below="@id/containerscroll">
                <TextView
                    android:text="@string/home_top_title"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/texttop"
                    android:textColor="@android:color/background_dark"
                    android:fadeScrollbars="true"
                    android:typeface="serif"
                    android:textDirection="ltr"
                    android:layout_gravity="center"
                    android:foregroundGravity="center"
                    android:gravity="center"
                    android:layout_marginTop="@dimen/abc_action_bar_content_inset_material"
                    android:textSize="@dimen/abc_action_button_min_height_material"
                    android:textStyle="bold" />
                <LinearLayout
                    android:orientation="horizontal"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/linearLayout1"
                    android:layout_marginTop="@dimen/abc_action_bar_content_inset_with_nav"
                    android:gravity="center">
                    <Button
                        android:layout_width="137.5dp"
                        android:layout_height="match_parent"
                        android:id="@+id/btnstart"
                        android:layout_marginEnd="@dimen/abc_action_bar_default_height_material"
                        android:background="@android:color/background_light"
                        android:text="@string/home_top_servicesbtn" />
                    <Button
                        android:text="@string/home_top_featuresbtn"
                        android:layout_width="132.0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/btnfeatures"
                        android:background="@android:color/background_light" />
                </LinearLayout>
            </LinearLayout>

            <Space
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/hometopspace"
                android:layout_below="@id/toplayout"
                android:importantForAutofill="yes"
                android:background="@android:color/black"
                android:minHeight="@dimen/abc_action_bar_content_inset_material" />

            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/homeserviceslayout"
                android:fadingEdge="none"
                android:textDirection="ltr"
                android:layoutDirection="ltr"
                android:padding="15dp"
                android:layout_marginBottom="0.0dp"
                android:background="#ffffffff"
                android:layout_below="@id/hometopspace">
                <TextView
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/homwtextservices"
                    android:textColor="#000"
                    android:fadeScrollbars="true"
                    android:typeface="serif"
                    android:textDirection="ltr"
                    android:layout_gravity="center"
                    android:foregroundGravity="center"
                    android:gravity="center"
                    android:text="@string/home_services_title" />
                <TextView
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/homeservicestextparagraph"
                    android:text="@string/home_services_text"
                    android:typeface="normal"
                    android:justificationMode="inter_word"
                    android:keyboardNavigationCluster="true"
                    android:textAlignment="viewStart"
                   />
                <TextView
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvhomeservicesp"
                    android:text="@string/home_services_text2"
                    android:textAlignment="viewStart"
                    android:justificationMode="inter_word" />
            </LinearLayout>
            <Space
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/homeservicesspace"
                android:layout_below="@id/homeserviceslayout"
                android:importantForAutofill="auto"
                android:minHeight="@dimen/abc_action_bar_content_inset_material" />
            <GridLayout
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/homeservicesgrid"
                android:columnCount="3"
                android:rowCount="3"
                android:textAlignment="center"
                android:layout_below="@id/homeservicesspace"
                android:background="#ffffffff"
                android:foregroundGravity="fill_vertical"
                android:useDefaultMargins="true"
                android:padding="@dimen/abc_action_bar_elevation_material">
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout4"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_gravity="left|fill"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView1"
                        android:src="@mipmap/ic_designweb"
                        android:layout_gravity="left"
                        android:contentDescription="@string/title_home" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView1"
                        android:text="@string/home_services_cat1"
                        android:foregroundGravity="center"
                        android:layout_gravity="center"
                        android:textAlignment="center"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="fill">
                    <ImageView
                        android:src="@mipmap/ic_developmentweb"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView2"
                        android:contentDescription="@string/title_home" />
                    <TextView
                        android:text="@string/home_services_cat2"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView2"
                        android:layout_gravity="center"
                        android:foregroundGravity="center"
                        android:textAlignment="center"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:gravity="fill">
                    <ImageView
                        android:src="@mipmap/ic_mobile"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView3"
                        android:layout_gravity="center_horizontal"
                        android:contentDescription="@string/title_home" />
                    <TextView
                        android:text="@string/home_services_cat3"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView3"
                        android:foregroundGravity="center"
                        android:layout_gravity="center"
                        android:textAlignment="center"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout5"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_gravity="left">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView1"
                        android:src="@mipmap/ic_digitalmarketing"
                        android:layout_gravity="left"
                        android:contentDescription="@string/title_home" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView1"
                        android:text="@string/home_services_cat4"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:textAlignment="center"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout6"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_gravity="center">
                    <ImageView
                        android:src="@mipmap/ic_designgraphic"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageView2"
                        android:contentDescription="@string/title_home" />
                    <TextView
                        android:text="@string/home_services_cat5"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView2"
                        android:layout_gravity="center"
                        android:foregroundGravity="center"
                        android:textAlignment="center"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout7"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content">
                    <ImageView
                        android:src="@mipmap/ic_hostingdomains"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageView3"
                        android:layout_gravity="center_horizontal"
                        android:contentDescription="@string/title_home" />
                    <TextView
                        android:text="@string/home_services_cat6"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView3"
                        android:foregroundGravity="center"
                        android:gravity="center"
                        android:textAlignment="center"/>
                </LinearLayout>
            </GridLayout>
            <Space
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/homehowitworkssspace"
                android:importantForAutofill="auto"
                android:minHeight="@dimen/abc_action_bar_content_inset_material"
                android:layout_toLeftOf="@id/toplayout"
                android:layout_below="@id/homeservicesgrid" />
            <RelativeLayout
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/relativeLayout1"
                android:layout_below="@id/homehowitworkssspace"
                android:background="#ffffffff">
                <TextView
                    android:text="@string/home_howitworks_title"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView4"
                    android:textAlignment="center"
                    android:padding="5dp"
                    android:paddingBottom="@dimen/abc_action_bar_content_inset_material"
                    android:layout_alignParentTop="true"
                    android:textColor="@android:color/background_dark" />
                <CheckBox
                    android:text="@string/home_howitworks_note1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/checkBox1"
                    android:checked="true"
                    android:layout_below="@id/textView4"
                    android:button="@drawable/ic_mtrl_chip_checked_black"
                    android:padding="@dimen/abc_action_bar_icon_vertical_padding_material"
                    android:layout_marginStart="@dimen/abc_action_bar_elevation_material" />
                <CheckBox
                    android:text="@string/home_howitworks_note2"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/checkBox2"
                    android:checked="true"
                    android:layout_below="@id/checkBox1"
                    android:button="@drawable/ic_mtrl_chip_checked_black"
                    android:padding="@dimen/abc_action_bar_icon_vertical_padding_material"
                    android:layout_marginStart="@dimen/abc_action_bar_elevation_material" />
                <CheckBox
                    android:text="@string/home_howitworks_note3"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/checkBox3"
                    android:checked="true"
                    android:layout_below="@id/checkBox2"
                    android:button="@drawable/ic_mtrl_chip_checked_black"
                    android:padding="@dimen/abc_action_bar_icon_vertical_padding_material"
                    android:layout_marginStart="@dimen/abc_action_bar_elevation_material" />
                <CheckBox
                    android:text="@string/home_howitworks_note4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/checkBox5"
                    android:checked="true"
                    android:layout_below="@id/checkBox3"
                    android:button="@drawable/ic_mtrl_chip_checked_black"
                    android:padding="@dimen/abc_action_bar_icon_vertical_padding_material"
                    android:layout_marginStart="@dimen/abc_action_bar_elevation_material" />
                <CheckBox
                    android:text="@string/home_howitworks_note5"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/checkBox6"
                    android:checked="true"
                    android:layout_below="@id/checkBox5"
                    android:button="@drawable/ic_mtrl_chip_checked_black"
                    android:padding="@dimen/abc_action_bar_icon_vertical_padding_material"
                    android:layout_marginStart="@dimen/abc_action_bar_elevation_material" />
            </RelativeLayout>
            <Space
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/homevideoshowsspace"
                android:importantForAutofill="auto"
                android:minHeight="@dimen/abc_action_bar_content_inset_material"
                android:layout_below="@id/relativeLayout1" />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/relativeLayout3"
                android:layout_below="@id/homevideoshowsspace"
                android:background="@android:color/background_light">
                <TextView
                    android:text="@string/home_videoshow_title"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView5"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:padding="@dimen/abc_action_bar_icon_vertical_padding_material"
                    android:paddingBottom="@dimen/abc_action_bar_icon_vertical_padding_material"
                    android:textColor="@android:color/background_dark"
                    android:textAlignment="center" />
            </RelativeLayout>
            <LinearLayout
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/gridcounter"
                android:layout_below="@id/relativeLayout3"
                android:background="@android:color/background_light"
                android:importantForAutofill="yes"
                android:orientation="horizontal"
                android:gravity="center"
                android:textAlignment="center"
                android:layout_marginBottom="0.0dp"
                android:padding="@dimen/abc_action_bar_icon_vertical_padding_material">
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout9"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:paddingEnd="@dimen/abc_action_bar_icon_vertical_padding_material"
                    android:importantForAutofill="auto"
                    android:textAlignment="center">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView5"
                        android:contentDescription="@string/title_home"
                        android:src="@mipmap/ic_apps"
                        android:paddingEnd="@dimen/abc_action_bar_overflow_padding_end_material" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView7"
                        android:foregroundGravity="center"
                        android:layout_gravity="center"
                        android:textAlignment="center"
                        android:text="@string/home_videoshow_apps"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView10"
                        android:foregroundGravity="center"
                        android:layout_gravity="center"
                        android:textAlignment="center"
                        android:text="@string/home_videoshow_appsnumber"
                        android:textColor="@android:color/background_dark"
                        android:textStyle="bold" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout3"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_margin="@dimen/abc_action_bar_elevation_material"
                    android:paddingEnd="@dimen/abc_dialog_padding_material"
                    android:importantForAutofill="auto"
                    android:textAlignment="center">
                    <ImageView
                        android:src="@mipmap/ic_progs"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView2"
                        android:contentDescription="@string/title_home"
                        android:paddingEnd="@dimen/abc_action_bar_overflow_padding_end_material" />
                    <TextView
                        android:text="@string/home_videoshow_progs"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView2"
                        android:layout_gravity="center"
                        android:foregroundGravity="center"
                        android:textAlignment="center"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView7"
                        android:foregroundGravity="center"
                        android:layout_gravity="center"
                        android:textAlignment="center"
                        android:text="@string/home_videoshow_progsnumber"
                        android:textColor="@android:color/background_dark"
                        android:textStyle="bold" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_margin="@dimen/abc_action_bar_elevation_material"
                    android:paddingEnd="@dimen/abc_dialog_padding_material"
                    android:importantForAutofill="auto"
                    android:textAlignment="center">
                    <ImageView
                        android:src="@mipmap/ic_clients"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView3"
                        android:contentDescription="@string/title_home"
                        android:paddingEnd="@dimen/abc_action_bar_overflow_padding_end_material" />
                    <TextView
                        android:text="@string/home_videoshow_clients"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView3"
                        android:foregroundGravity="center"
                        android:layout_gravity="center"
                        android:textAlignment="center"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView7"
                        android:foregroundGravity="center"
                        android:layout_gravity="center"
                        android:textAlignment="center"
                        android:text="@string/home_videoshow_clientsnumber"
                        android:textColor="@android:color/background_dark"
                        android:textStyle="bold" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:id="@+id/linearLayout11"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_margin="@dimen/abc_action_bar_elevation_material"
                    android:importantForAutofill="auto"
                    android:textAlignment="center">
                    <ImageView
                        android:src="@mipmap/ic_testmonials"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView3"
                        android:contentDescription="@string/title_home"
                        android:paddingEnd="@dimen/abc_action_bar_icon_vertical_padding_material" />
                    <TextView
                        android:text="@string/home_videoshow_testmonials"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView3"
                        android:foregroundGravity="center"
                        android:layout_gravity="center"
                        android:textAlignment="center"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView7"
                        android:foregroundGravity="center"
                        android:layout_gravity="center"
                        android:textAlignment="center"
                        android:text="@string/home_videoshow_testmonialsnumber"
                        android:textColor="@android:color/background_dark"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>
            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/homemobilephotosspace"
                android:importantForAutofill="auto"
                android:minHeight="@dimen/abc_action_bar_content_inset_material"
                android:layout_below="@id/gridcounter"
                android:layout_toLeftOf="@id/toplayout" />
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout12"
                android:layout_below="@id/homemobilephotosspace"
                android:background="@android:color/background_light">
                <TextView
                    android:text="@string/home_followusweb_title"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView8"
                    android:textAlignment="center"
                    android:textColor="@android:color/background_dark" />
                <ImageView
                    android:src="@mipmap/placeit"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView6"
                    android:contentDescription="@string/app_name" />
                <TextView
                    android:text="@string/home_followusweb_url"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView12"
                    android:textAlignment="center"
                    android:textColor="@android:color/background_dark"
                    android:clickable="true" />
            </LinearLayout>
            <Space
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/homesocialmediaspace"
                android:importantForAutofill="auto"
                android:minHeight="@dimen/abc_action_bar_content_inset_material"
                android:layout_below="@id/linearLayout12"
                android:layout_toLeftOf="@id/toplayout" />
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout7"
                android:layout_below="@id/homesocialmediaspace"
                android:layout_marginTop="@dimen/abc_action_bar_icon_vertical_padding_material"
                android:gravity="center">
                <TextView
                    android:text="@string/home_followusweb_title"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView111"
                    android:textAlignment="center"
                    android:textColor="@android:color/background_dark"
                    android:includeFontPadding="false"
                    android:paddingVertical="@dimen/abc_action_bar_icon_vertical_padding_material"
                    android:layout_gravity="center"
                    android:padding="@dimen/abc_action_bar_elevation_material" />
                <ImageButton
                    android:src="@mipmap/ic_insta"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton1"
                    android:contentDescription="@string/app_name"
                    android:cropToPadding="false"
